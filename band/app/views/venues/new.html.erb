<%= form_for(@venue) do |f| %>
	<%= f.label :name %>
	<%= f.text_field :name %>
	<%= f.label :city %>
	<%= f.text_field :city %>
	<%= f.label :state %>
	<%= f.text_field :state %>
	<%= f.label :family_friendly %>
	<%= f.radio_button :family_friendly, 'True' %>
	<%= f.radio_button :family_friendly, 'False' %>	
	<%= f.submit %>
<% end %>