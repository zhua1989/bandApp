<html>
<head>
	<title></title>
</head>
<body>
<h1>Hey this is the index page for the events</h1>
	
	<%= form_tag("/search", method: "get") do %>
	  <%= label_tag(:q, "Search for:") %>

		<% @events.each do |event| %>
		<%= link_to event.date, event_path(event) %>

		<% end %>

	  <%= submit_tag("Search") %>
	<% end%>




</body>
</html>